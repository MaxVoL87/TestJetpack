                android:scaleX="0.88"
                android:scaleY="0.88"
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/vEarth"/>

            <KeyPosition
                motion:framePosition="40"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/vEat"
                motion:percentX="-0.14"
                motion:percentY="0.078" />

            <KeyPosition
                motion:framePosition="55"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/vEat"
                motion:percentX="0.05"
                motion:percentY="0.078" />

            <KeyAttribute
                android:scaleX="0.88"
                android:scaleY="0.88"
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/vEat"/>
        </KeyFrameSet>
    </Transition>

